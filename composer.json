{
  "name": "contributte/translation",
  "description": "Symfony/Translation integration for Nette Framework.",
  "type": "library",
  "keywords": [
    "symfony",
    "nette",
    "translation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/contributte/translation",
  "authors": [
    {
      "name": "Ales Wita"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "latte/latte": "^2.6|^3.0",
    "nette/di": "^3.0.6",
    "nette/finder": "^2.5.2",
    "nette/http": "^3.0.7",
    "nette/neon": "^3.3.1",
    "nette/schema": "^1.0",
    "nette/routing": "^3.0",
    "nette/utils": "^3.2.1",
    "symfony/translation": "^6.0",
    "symfony/config": "^6.0"
  },
  "require-dev": {
    "doctrine/orm": "^2.8",
    "mockery/mockery": "^1.4",
    "nette/application": "^3.1.0",
    "nette/bootstrap": "^3.0",
    "nette/database": "^3.1.1",
    "nette/robot-loader": " ^3.4.0",
    "nette/tester": "^2.3.1",
    "ninjify/nunjuck": "^0.3.0",
    "ninjify/qa": "^0.13",
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.1",
    "psr/log": "^1.1",
    "tracy/tracy": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Translation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/Tests",
      "Tests\\Fixtures\\": "tests/Fixtures",
      "Tests\\Toolkit\\": "tests/Toolkit"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
